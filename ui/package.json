    "scripts": {
        "start": "yarn run start:dev",
        "start:dev": "yarn run tailwind:watch & yarn run craco:start",
        "craco:start": "cross-env FAST_REFRESH=false craco start",
        "build": "yarn run build:css && cross-env FAST_REFRESH=false craco build",
        "build:docker": "yarn run build:css && cross-env NODE_ENV=development FAST_REFRESH=false craco build",
        "build:css": "npx @tailwindcss/cli -i src/styles/tailwind.css -o src/styles/generated/tailwindcss.css --minify --optimize",
        "tailwind:watch": "npx @tailwindcss/cli -i src/styles/tailwind.css -o src/styles/generated/tailwindcss.css --watch --poll",
        "test": "craco test",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx",
        "lint": "yarn eslint src",
        "lint:fix": "yarn eslint --fix src",
        "lint:css": "stylelint src/**/*.css",
        "prettify": "prettier --write",
        "test:coverage": "yarn test --watchAll=false --coverage",
        "release": "standard-version"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "public/**/*",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/messages.ts",
            "!src/reportWebVitals.ts",
            "!src/**/*/types.ts",
            "!src/index.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    },
